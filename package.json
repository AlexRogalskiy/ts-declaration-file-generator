{
  "name": "ts-declaration-file-generator",
  "version": "1.2.0",
  "description": "Generates a typescript declaration file using runtime analysis information.",
  "bin": {
    "dts-generate": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "node dist/index.js",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "test": "jest src/",
    "test:clear-cache": "jest --clearCache",
    "test:e2e": "jest e2e/",
    "lint": "eslint --max-warnings 0 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "script": "ts-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proglang/ts-declaration-file-generator.git"
  },
  "keywords": [
    "typescript",
    "declaration",
    "files"
  ],
  "author": "fcristiani",
  "license": "MIT",
  "homepage": "https://github.com/proglang/ts-declaration-file-generator",
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "typescript": "^3.9.7",
    "command-line-args": "^5.0.2"
  },
  "engines": {
    "node": ">=10.14.2"
  }
}
