{
  "name": "ts-declaration-file-generator",
  "version": "1.0.1",
  "description": "Generates a typescript declaration file using runtime analysis information.",
  "bin": {
    "dts-generate": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "node dist/index.js",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "prebuild": "rm -rf dist",
    "build": "npx tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proglang/ts-declaration-file-generator.git"
  },
  "keywords": [
    "typescript",
    "declaration",
    "files"
  ],
  "author": "fcristiani",
  "license": "MIT",
  "homepage": "https://bitbucket.org/fcristiani/tsd-generator#readme",
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "command-line-args": "^5.0.2"
  },
  "engines": {
    "node": ">=10.14.2"
  }
}
